<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="project.project_model_view_form" model="ir.ui.view">
    <field name="name">project.project.form</field>
    <field name="model">project.project.model</field>
    <field name="inherit_id" ref="project.project_model_view_form"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='description']" position="after">
          <field name="startup"/>
        </xpath>
    </field>
</record>

</odoo>